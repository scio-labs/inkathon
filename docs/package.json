{
  "name": "docs",
  "version": "6.1.1",
  "private": true,
  "engines": {
    "node": ">=20 <=24"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build --turbo",
    "start": "next start",
